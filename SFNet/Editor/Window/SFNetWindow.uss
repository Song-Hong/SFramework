.sfnet-background {
    flex-grow: 1;
    background-color: rgb(56, 56, 56);
    flex-direction: row;
}

.sfnet-table_container {
    width: 120px;
    align-items: center;
}

.sfnet-table_title {
    margin-top: 8px;
    margin-right: 8px;
    margin-bottom: 8px;
    margin-left: 8px;
    padding-top: 8px;
    padding-right: 0;
    padding-bottom: 8px;
    padding-left: 0;
    color: rgb(255, 255, 255);
    font-size: 16px;
    -unity-text-align: middle-center;
}

.sfnet-content_container {
    flex-grow: 1;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
}

.sfnet-content_content_view {
    flex-grow: 1;
    background-color: rgb(32, 32, 32);
